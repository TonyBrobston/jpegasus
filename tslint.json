{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "comment-type": [true],
        "quotemark": ["single"],
        "one-variable-per-declaration": false,
        "no-bitwise": false,
        "interface-name": [true, "never-prefix"],
        "typedef": [true, "call-signature"]
    },
    "rulesDirectory": []
}