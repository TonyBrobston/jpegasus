{
  "name": "jpegasus",
  "version": "1.9.54",
  "description": "A client-side JavaScript tool that takes in a `File` of type `.jpeg`, `.png`, or `.gif` and returns a compressed `File` of type `.jpeg`.",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/TonyBrobston/jpegasus"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "version": "yarn build && git add .",
    "postversion": "git push && git push --tags",
    "coverage": "jest --coverage",
    "lint:js": "eslint --fix ./src ./tests",
    "test": "yarn coverage && yarn lint:js",
    "unit": "jest"
  },
  "keywords": [
    "javascript",
    "image",
    "jpeg",
    "gif",
    "png",
    "client",
    "client-side",
    "compression",
    "compressor",
    "canvas",
    "scale",
    "size",
    "reduce",
    "smaller"
  ],
  "author": "Tony Brobston",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/chance": "^1.0.1",
    "@types/jest": "^24.0.11",
    "chance": "^1.0.18",
    "eslint": "^5.14.1",
    "eslint-config-google": "^0.12.0",
    "jest": "^24.1.0",
    "typescript": "^3.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js"
    ],
    "coverageDirectory": ".coverage",
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "peerDependencies": {
    "core-js": "^3.0.0",
    "regenerator-runtime": "^0.13.2"
  }
}
