{
  "name": "jpegasus",
  "version": "1.7.4",
  "description": "A client-side JavaScript tool that takes in a `File` of type `.jpeg`, `.png`, or `.gif` and returns a compressed `File` of type `.jpeg`.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/TonyBrobston/jpegasus"
  },
  "scripts": {
    "create": "babel src -d lib",
    "coverage": "jest --coverage",
    "deploy:major": "npm login && yarn build && npm version major && npm publish",
    "deploy:minor": "npm login && yarn build && npm version minor && npm publish",
    "deploy:patch": "npm login && yarn build && npm version patch && npm publish",
    "build": "rm -rf lib && babel src -d lib",
    "unit": "jest"
  },
  "keywords": [
    "javascript",
    "image",
    "jpeg",
    "gif",
    "png",
    "client",
    "client-side",
    "compression",
    "compressor",
    "canvas",
    "scale",
    "size",
    "reduce",
    "smaller"
  ],
  "author": "Tony Brobston",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chance": "^1.0.18",
    "jest": "^23.6.0"
  },
  "dependencies": {
    "base64toblob": "^0.0.2",
    "exif-js": "^2.3.0"
  },
  "jest": {
    "coverageDirectory": ".coverage",
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
