{
  "name": "jpegasus",
  "version": "1.11.1",
  "description": "A client-side JavaScript tool that takes in a `File` of type `.jpeg`, `.png`, or `.gif` and returns a compressed `File` of type `.jpeg`.",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/TonyBrobston/jpegasus"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "version": "yarn build && git add .",
    "postversion": "git push && git push --tags",
    "coverage": "jest --coverage",
    "lint:ts": "tslint --fix -c tslint.json 'src/**/*.ts' 'tests/**/*.test.ts'",
    "test": "yarn lint:ts && yarn coverage && yarn acceptance",
    "acceptance": "./.scripts/runAcceptanceTests.sh",
    "unit": "npx jest"
  },
  "keywords": [
    "javascript",
    "image",
    "jpeg",
    "gif",
    "png",
    "client",
    "client-side",
    "client side",
    "browser",
    "dom",
    "compress",
    "file",
    "blob",
    "compression",
    "compressor",
    "canvas",
    "scale",
    "size",
    "reduce",
    "smaller"
  ],
  "author": "Tony Brobston",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/chance": "^1.0.6",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "chance": "^1.0.18",
    "cypress": "^3.4.1",
    "cypress-file-upload": "^3.3.4",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.1.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2",
    "wait-on": "^3.3.0"
  },
  "peerDependencies": {
    "core-js": "^3.0.0",
    "regenerator-runtime": "^0.13.2"
  }
}
