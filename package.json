{
  "name": "jpegasus",
  "version": "1.11.2",
  "description": "A client-side JavaScript tool that takes in a `File` of type `.jpeg`, `.png`, or `.gif` and returns a compressed `File` of type `.jpeg`.",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/TonyBrobston/jpegasus"
  },
  "scripts": {
    "acceptance": "./.scripts/runAcceptanceTests.sh",
    "acceptance:interactive": "./.scripts/runAcceptanceTestsInteractive.sh",
    "build": "rm -rf lib && tsc",
    "coverage": "jest --coverage",
    "documentation": "typedoc --readme none --theme vuepress --plugin typedoc-plugin-markdown --hideGenerator",
    "lint:ts": "tslint --fix -c tslint.json 'src/**/*.ts' 'tests/**/*.test.ts'",
    "postversion": "git push && git push --tags",
    "test": "yarn lint:ts && yarn coverage && yarn acceptance",
    "unit": "npx jest",
    "version": "yarn build && git add ."
  },
  "keywords": [
    "javascript",
    "image",
    "jpeg",
    "gif",
    "png",
    "client",
    "client-side",
    "client side",
    "browser",
    "dom",
    "compress",
    "file",
    "blob",
    "compression",
    "compressor",
    "canvas",
    "scale",
    "size",
    "reduce",
    "smaller"
  ],
  "author": "Tony Brobston",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/chance": "^1.0.6",
    "@types/jest": "^24.0.18",
    "@types/node": "^13.5.0",
    "chance": "^1.0.18",
    "commitlint": "^8.2.0",
    "cypress": "^3.4.1",
    "cypress-file-upload": "^3.3.4",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-canvas-mock": "^2.1.1",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typedoc": "^0.16.9",
    "typedoc-plugin-markdown": "^2.2.16",
    "typescript": "^3.6.2",
    "wait-on": "^4.0.0"
  },
  "peerDependencies": {
    "core-js": "^3.3.5",
    "regenerator-runtime": "^0.13.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
